<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.ManageStockClientController">
   <children>
      <Pane fx:id="InnerPaneInManageStock" layoutX="23.0" layoutY="21.0" prefHeight="543.0" prefWidth="738.0" />
      <TextField fx:id="tfEnterNewCopyID" disable="true" layoutX="596.0" layoutY="514.0" prefHeight="34.0" prefWidth="95.0" promptText="enter new copy ID" />
      <Button fx:id="btnAddNewCopy" disable="true" layoutX="685.0" layoutY="509.0" mnemonicParsing="false" onAction="#onClickAddCopy" prefHeight="46.0" prefWidth="50.0" styleClass="btnWithImage" stylesheets="@../styles/mainView.css">
         <graphic>
            <ImageView fitHeight="36.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resources/button/plus.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="btnAddNewBook" layoutX="691.0" layoutY="27.0" mnemonicParsing="false" onAction="#onClickAddNewBook" prefHeight="44.0" prefWidth="58.0" styleClass="btnWithImage" stylesheets="@../styles/mainView.css">
         <graphic>
            <ImageView fitHeight="46.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resources/button/addBook.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <TableView fx:id="tvCopies" layoutX="596.0" layoutY="93.0" onMouseClicked="#onChosenRow" prefHeight="414.0" prefWidth="156.0">
        <columns>
          <TableColumn fx:id="tvColumsCopyID" prefWidth="165.0" text="Copy ID" />
        </columns>
      </TableView>
      <Button fx:id="btnDeleteCopy" disable="true" layoutX="717.0" layoutY="513.0" mnemonicParsing="false" onAction="#onClickDeleteCopy" prefHeight="34.0" prefWidth="58.0" styleClass="btnWithImage" stylesheets="@../styles/mainView.css">
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resources/button/delete1.png" />
               </image>
            </ImageView>
         </graphic></Button>
   </children>
</AnchorPane>

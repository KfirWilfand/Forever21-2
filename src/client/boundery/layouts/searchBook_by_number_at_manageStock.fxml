<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="543.0" prefWidth="578.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.SearchBookOnManageStockController">
   <children>
      <TextField fx:id="tfCatalogNumberForSearch" layoutX="17.0" layoutY="17.0" promptText="Enter Catalog Number" />
      <Button fx:id="btnSearchBook" layoutX="179.0" layoutY="17.0" mnemonicParsing="false" onAction="#onClickSearchBook" text="Search" />
      <TableView fx:id="booksTable" layoutX="14.0" layoutY="57.0" prefHeight="475.0" prefWidth="545.0">
         <columns>
            <TableColumn prefWidth="109.0" text="Catalog Number" />
            <TableColumn prefWidth="104.0" text="Book Name" />
            <TableColumn prefWidth="105.0" text="Author Name" />
            <TableColumn prefWidth="115.0" text="Copies Number" />
            <TableColumn prefWidth="112.0" text="Edition Number" />
         </columns>
      </TableView>
   </children>
</Pane>
